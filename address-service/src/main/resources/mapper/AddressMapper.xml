<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.douyinmall.address.mapper.AddressMapper">

        <update id="update" parameterType="com.douyinmall.address.domain.VO.Address">
                UPDATE address
                <set>
                        <if test="userId != null">
                                user_id = #{userId},
                        </if>
                        <if test="consignee != null">
                                consignee = #{consignee},
                        </if>
                        <if test="phone != null">
                                phone = #{phone},
                        </if>
                        <if test="sex != null">
                                sex = #{sex},
                        </if>
                        <if test="provinceName != null">
                                province_name = #{provinceName},
                        </if>
                        <if test="cityName != null">
                                city_name = #{cityName},
                        </if>
                        <if test="districtName != null">
                                district_name = #{districtName},
                        </if>
                        <if test="detail != null">
                                detail = #{detail},
                        </if>
                        <if test="isDefault != null">
                                is_default = #{isDefault}
                        </if>
                </set>
                WHERE id = #{id}
        </update>
</mapper>
